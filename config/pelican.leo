    def __init__(self, importCommands):
        '''{|{x=cap_name}|}_Importer.__init__'''
        super().__init__(
            importCommands,
            language = '{|{x=name}|}',
            state_class = {|{x=cap_name}|}_ScanState,
            strict = &lt;|True leading whitespace is significant. Otherwise False|&gt;,
        )
        
    @others
